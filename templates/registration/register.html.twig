{% extends "base.html.twig" %}

{% block title %}Inscription{% endblock %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.password.first) }}
    {{ form_row(form.password.second) }}
    <button type="submit">Create</button>
    {{ form_end(form) }}
{% endblock %}